

<h2>Recentely added</h2>
<div>
  <% books.forEach(book => { %>
  <div>
    <a href="/books/<%= book.id %>">
      <img
        src=" <%= book.coverImageName.path %> "
        alt=""
        height="350"
        width="450"
      />
    </a>
    <h2><%= book.title %></h2>
  </div>
  <% }) %>
</div>

